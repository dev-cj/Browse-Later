{
    "name": "Browse Later",
    "version": "1.0.2",
    "manifest_version": 2,
    "description": "Save links to browse later.",
    "browser_action": {
        "default_icon": "defaulticon.png",
        "default_popup": "popup.html"
    },
    "short_name": "Save Links to Browse Later",
    "icons": {
        "128": "icon128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "storage",
        "activeTab"
    ]
}